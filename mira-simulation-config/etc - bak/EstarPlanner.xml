<root>
	<Planner>
		<!--Use double buffering for the planner map-->
		<UseDoubleBuffer>true</UseDoubleBuffer>
		<!--How long should we plan until we decide that we can not plan a path-->
		<MaxPlanningTime>30000</MaxPlanningTime>
		<!--If the planner has no path since the specified time (but had one before) XPathLost is thrown-->
		<PathLostTimeout>10000</PathLostTimeout>
		<!--The color scaling of the path costs during visualization-->
		<Navfscale>0.02</Navfscale>
	</Planner>
</root>