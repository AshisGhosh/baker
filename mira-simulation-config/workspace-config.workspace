                <!--Show slot count-->
                <ShowSlotCount>false</ShowSlotCount>
                <!--Show last update time-->
                <ShowLastUpdate>false</ShowLastUpdate>
                <!--Show publishers/subscribers-->
                <ShowPublisherSubscriber>false</ShowPublisherSubscriber>
                <!--Automatically hide the filter bar-->
                <HideFilterBarAct>false</HideFilterBarAct>
              </Part>
            </item>
            <item>
              <Name>Properties [DriveView]</Name>
              <Part class="mira::PropertyView">
                <HideFilterBarAct>false</HideFilterBarAct>
              </Part>
            </item>
            <item>
              <Name>Authorities</Name>
              <Part class="mira::AuthorityView">
